{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=1536' next build",
    "start": "next start",
    "server": "NODE_OPTIONS='--max-old-space-size=1536' next build && next start",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "lint:report": "eslint \"src/**/*.{js,ts,tsx}\" -f html -o eslint-report.html",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,css,md}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
