    "@svgr/webpack": "^8.1.0",
    "@tabler/icons-react": "^3.2.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.13.8",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.4",
    "canvas": "^2.11.2",
    "color-thief-react": "^2.1.0",
    "date-and-time": "^3.5.0",
    "eslint-formatter-html": "^2.7.2",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
